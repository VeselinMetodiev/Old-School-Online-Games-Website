<!DOCTYPE html>
<html lang="en">
  <%- include('includes/head') %>
  <body>
    <div class="container">
      <%- include('includes/header') %>
      <div class="content-wrapper">
        <div class="left-column">
          <div class="box">
            <div class="photos-main-title">Снимки</div>
            <ul class="photos-categories">
              <li class="active"><a href="#">Всички</a></li>
              <li><a href="#">Най-нови</a></li>
              <li><a href="#">Най-харесвани</a></li>
            </ul>

            <div class="photo-grid">
              <% photos.forEach(function(photo) { %>
              <div class="photo-item">
                <a href="#">
                  <img
                    src="/uploads/<%= photo %>"
                    alt="A dynamically loaded photo"
                  />
                </a>
                <div class="photo-item-info">Photo Info<br />0 comments</div>
              </div>
              <% }); %>
            </div>
          </div>
        </div>

        <div class="right-column">
          <div class="box">
            <div class="box-title">Най-нови албуми</div>
            <ul class="new-albums">
              <li><a href="#">Моята колекция</a></li>
              <li><a href="#">Празник в града</a></li>
              <li><a href="#">Есенни пейзажи</a></li>
              <li><a href="#">Снимки от пътуване</a></li>
            </ul>
          </div>
          <div class="box">
            <div class="box-title">Потребители онлайн</div>
            <div class="online-users">
              <div class="user-icon">user1</div>
              <div class="user-icon">user2</div>
              <div class="user-icon">user3</div>
              <div class="user-icon">user4</div>
              <div class="user-icon">user5</div>
              <div class="user-icon">user6</div>
              <div class="user-icon">user7</div>
              <div class="user-icon">user8</div>
              <div class="user-icon">user9</div>
              <div class="user-icon">user10</div>
            </div>
          </div>
        </div>
      </div>

      <%- include('includes/footer') %>
    </div>
  </body>
</html>
